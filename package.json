{
  "name": "rule-34-pwa-original-api",
  "description": "A wrapper for all rule34 related sites (Danbooru based)",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "nodemon -q index.js",
    "dev": "cross-env NODE_ENV=development DEBUG=* npm run start",
    "debug": "cross-env NODE_ENV=development DEBUG=* node --inspect index.js",
    "test:dev": "cross-env NODE_ENV=development jest",
    "test:prod": "cross-env NODE_ENV=production jest"
  },
  "dependencies": {
    "apicache": "^1.5.3",
    "body-parser": "^1.19.0",
    "camaro": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "~4.17.1",
    "express-rate-limit": "^5.1.1",
    "express-validator": "^6.4.0",
    "he": "^1.2.0",
    "helmet": "^3.21.3",
    "module-alias": "^2.2.2",
    "morgan": "~1.10.0",
    "newrelic": "^6.5.0",
    "node-fetch": "^2.6.0",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.1",
    "supertest": "^4.0.2"
  },
  "_moduleAliases": {
    "@root": ".",
    "@server": "server/",
    "@assets": "assets/",
    "@boorus": "server/routes/boorus"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "@server/(.*)": "<rootDir>/server/$1",
      "@assets/(.*)": "<rootDir>/assets/$1",
      "@boorus/(.*)": "<rootDir>/server/routes/boorus/$1"
    }
  }
}
